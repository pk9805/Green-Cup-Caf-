<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 800" width="1200" height="800">
  <!-- Background Gradient -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#2E7D32;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1B5E20;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="1200" height="800" fill="url(#bgGradient)" />
  
  <!-- Coffee Bean Pattern -->
  <g opacity="0.1">
    <!-- Row 1 -->
    <ellipse cx="100" cy="100" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,100,100)" />
    <ellipse cx="300" cy="100" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,300,100)" />
    <ellipse cx="500" cy="100" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,500,100)" />
    <ellipse cx="700" cy="100" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,700,100)" />
    <ellipse cx="900" cy="100" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,900,100)" />
    <ellipse cx="1100" cy="100" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,1100,100)" />
    
    <!-- Row 2 -->
    <ellipse cx="200" cy="200" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,200,200)" />
    <ellipse cx="400" cy="200" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,400,200)" />
    <ellipse cx="600" cy="200" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,600,200)" />
    <ellipse cx="800" cy="200" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,800,200)" />
    <ellipse cx="1000" cy="200" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,1000,200)" />
    
    <!-- Row 3 -->
    <ellipse cx="100" cy="300" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,100,300)" />
    <ellipse cx="300" cy="300" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,300,300)" />
    <ellipse cx="500" cy="300" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,500,300)" />
    <ellipse cx="700" cy="300" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,700,300)" />
    <ellipse cx="900" cy="300" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,900,300)" />
    <ellipse cx="1100" cy="300" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,1100,300)" />
    
    <!-- Row 4 -->
    <ellipse cx="200" cy="400" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,200,400)" />
    <ellipse cx="400" cy="400" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,400,400)" />
    <ellipse cx="600" cy="400" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,600,400)" />
    <ellipse cx="800" cy="400" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,800,400)" />
    <ellipse cx="1000" cy="400" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,1000,400)" />
    
    <!-- Row 5 -->
    <ellipse cx="100" cy="500" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,100,500)" />
    <ellipse cx="300" cy="500" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,300,500)" />
    <ellipse cx="500" cy="500" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,500,500)" />
    <ellipse cx="700" cy="500" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,700,500)" />
    <ellipse cx="900" cy="500" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,900,500)" />
    <ellipse cx="1100" cy="500" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,1100,500)" />
    
    <!-- Row 6 -->
    <ellipse cx="200" cy="600" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,200,600)" />
    <ellipse cx="400" cy="600" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,400,600)" />
    <ellipse cx="600" cy="600" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,600,600)" />
    <ellipse cx="800" cy="600" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,800,600)" />
    <ellipse cx="1000" cy="600" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,1000,600)" />
    
    <!-- Row 7 -->
    <ellipse cx="100" cy="700" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,100,700)" />
    <ellipse cx="300" cy="700" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,300,700)" />
    <ellipse cx="500" cy="700" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,500,700)" />
    <ellipse cx="700" cy="700" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,700,700)" />
    <ellipse cx="900" cy="700" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,900,700)" />
    <ellipse cx="1100" cy="700" rx="20" ry="30" fill="#FFFFFF" transform="rotate(45,1100,700)" />
  </g>
  
  <!-- Central Coffee Cup -->
  <g transform="translate(600,400) scale(2)">
    <!-- Cup -->
    <path d="M-40,0 L40,0 L35,60 L-35,60 Z" fill="#4CAF50" />
    
    <!-- Cup Handle -->
    <path d="M40,15 Q60,20 40,40" stroke="#4CAF50" stroke-width="5" fill="none" />
    
    <!-- Cup Rim -->
    <path d="M-40,0 L40,0" stroke="#2E7D32" stroke-width="3" stroke-linecap="round" />
    
    <!-- Steam -->
    <path d="M-20,-10 Q-10,-25 0,-10 Q10,-25 20,-10" stroke="#FFFFFF" stroke-width="3" fill="none" stroke-linecap="round" opacity="0.7" />
    
    <!-- Leaf Design on Cup -->
    <path d="M0,30 Q15,20 10,40 Q-5,30 0,30 Z" fill="#8BC34A" />
    
    <!-- Saucer -->
    <ellipse cx="0" cy="60" rx="45" ry="5" fill="#2E7D32" />
  </g>
  
  <!-- Decorative Elements -->
  <!-- Coffee Beans -->
  <g transform="translate(200,200) rotate(30)">
    <ellipse cx="0" cy="0" rx="30" ry="50" fill="#795548" opacity="0.6" />
    <path d="M0,-40 Q0,0 0,40" stroke="#5D4037" stroke-width="2" fill="none" />
  </g>
  
  <g transform="translate(1000,600) rotate(-20)">
    <ellipse cx="0" cy="0" rx="30" ry="50" fill="#795548" opacity="0.6" />
    <path d="M0,-40 Q0,0 0,40" stroke="#5D4037" stroke-width="2" fill="none" />
  </g>
  
  <!-- Plant Elements -->
  <g transform="translate(150,650)">
    <path d="M0,0 Q20,-40 40,-10 Q60,-50 80,-20 Q100,-60 120,-30" stroke="#8BC34A" stroke-width="5" fill="none" stroke-linecap="round" opacity="0.7" />
  </g>
  
  <g transform="translate(950,150)">
    <path d="M0,0 Q20,-40 40,-10 Q60,-50 80,-20 Q100,-60 120,-30" stroke="#8BC34A" stroke-width="5" fill="none" stroke-linecap="round" opacity="0.7" />
  </g>
</svg>